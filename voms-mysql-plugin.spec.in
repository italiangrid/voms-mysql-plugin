Summary: VOMS plugin for MySQL database
Name: @PACKAGE@
Version: @VERSION@
Release: 1_RH7.3
Source: %{name}-%{version}.tar.gz
Url: changeme
Copyright: changeme
Group: changeme
Packager: changeme
Prefix: %{_prefix}
BuildRoot: %{_tmppath}/%{name}-root

%description
Summary: VOMS plugin for MySQL database

%prep
%setup -q

%build
./configure @COMMAND_LINE@ --prefix=%{_prefix}
make

%install
rm -rf $RPM_BUILD_ROOT
make install DESTDIR=$RPM_BUILD_ROOT

%clean
rm -rf $RPM_BUILD_ROOT

%files
%defattr(-,root,root)
%{_prefix}/etc/voms/voms-mysql.data
%{_prefix}/etc/voms/voms-mysql-compat.data
%{_libdir}/libvomsmysql.a
%{_libdir}/libvomsmysql.la
%{_libdir}/libvomsmysql.so
%{_libdir}/libvomsmysql.so.0
%{_libdir}/libvomsmysql.so.0.0.0
